        function CheckRecurringPaymentStatusFunc(number, CRPSResponse) {
            if (number != null && number != "") {

                $.ajax({
                    url: '../LandingPage/CheckRecurringPaymentStatus?fbid=' + number,
                    method: 'get',
                    async: false,
                    success: function (rdata, textStatus, jqXHR) {
