        function gettingCkeyFromParramSession() {

            function getParameterByName(name, url = window.location.href) {
                name = name.replace(/[\[\]]/g, '\\$&');
                var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
                    results = regex.exec(url);
                if (!results) return null;
                if (!results[2]) return '';
                return decodeURIComponent(results[2].replace(/\+/g, ' '));
            }


            var ckey = getParameterByName('ckey');
            //console.log(ckey);



            if (ckey != null && typeof ckey != "undefined" && ckey != "null") {

            } else {
                ckey = getParameterByName('cKey');
            }

            if (ckey == null || typeof ckey == "undefined" || ckey == "null" || ckey == "") {
                ckey = sessionStorage.getItem("ckey");
            }

            if (ckey == null || typeof ckey == "undefined" || ckey == "null" || ckey == "") ckey = "000000";

            return ckey;

        }

