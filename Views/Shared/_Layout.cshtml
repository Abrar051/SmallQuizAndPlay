        function logout() {

            let fbid = sessionStorage.getItem('fbid');
            let username = sessionStorage.getItem("username");

            //$.ajax({
            //    type: 'GET',
            //    url: '../Home/Logout',
            //    success: function (data, status, xhr) {
            //        console.log('data: ', data);
            //    }
            //});

            $.post('../Home/Logout?userName=' + username + "&msisdn=" + fbid, function (data) { console.log(data) })

            $.get("../LandingPage/SaveCapmLog?msisdn=" + sessionStorage.getItem("fbid") + "&ckey=" + sessionStorage.getItem("ckey") + "&cname=" + "KidStar" + "&theme=Log_Out&srcurl=" + atob(sessionStorage.getItem("su")), function (rData) { });


